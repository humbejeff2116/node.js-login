



<%- include('header') %>






<h1 align="center">sign up</h1>
<div class="container">
<form action="/users/signup" method="POST" enctype="multipart/form-data">
    <label>name:</label>
    <input name="name" type="text" class="form-control "   autofocus>
    <label>username:</label>
    <input name="username" type="text" class="form-control" placeholder="username" >
    <label>bio:</label>
    <textarea name="bio" placeholder="enter your bio" class="form-control"></textarea>
    <label>profile image:</label>
    <input type="file" name="profileimage" class="form-control" > 
    <label>password:</label>
    <input name="password" type="password" class="form-control">
    <label>repeat password:</label>
    <input name="password2" type="password" class="form-control" ><br />
    <input type="submit" value="sign up" class="btn btn-primary ">
    </form>
</div>
   



<%- include('_footer') %> 